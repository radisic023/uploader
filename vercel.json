{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 10
            }
        }
    ],
    "routes": [
        {
            "src": "/styles.css",
            "dest": "/public/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "functions": {
        "server.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}
